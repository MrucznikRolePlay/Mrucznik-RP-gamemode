command_tickets_Impl(playerid)
{
    if(!IsAZasluzony(playerid) && PlayerInfo[playerid][pNewAP] == 0 && PlayerInfo[playerid][pAdmin] == 0 && !IsAZaufanyGracz(playerid)) return noAccessMessage(playerid);
    Support_ShowTickets(playerid);
    return 1;
}

