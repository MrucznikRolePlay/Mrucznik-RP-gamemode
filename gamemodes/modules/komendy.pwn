    if(Uprawnienia(playerid, ACCESS_KARY)) strcat(str, "{00FF00}+{FFFFFF} Panel kar\n");
    if(Uprawnienia(playerid, ACCESS_KARY_ZNAJDZ)) strcat(str, "\t» Wyszukiwanie kar\n");
