    new kasa = kaska[playerid];
    if(kasa < 1000) return PlayerPlaySound(playerid, 1085, 0.0, 0.0, 0.0);
    DajKase(playerid, -1000);
    PlayerPlaySound(playerid, 12201, 0.0, 0.0, 0.0);
    Sejf_Add(FRAC_KT, 1000);
    new str[64];
